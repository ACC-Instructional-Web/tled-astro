---
import OneColumn from './OneColumn.astro';
import TwoColumn from './TwoColumn.astro';
import ThreeColumn from './ThreeColumn.astro';
// import Accordion from './Accordion';
const { layouts } = Astro.props;

// http://localhost:4321/faculty-support/services/faculty-evaluation/ ACF One Column

// http://localhost:4321/faculty-support/services/teaching-and-learning-centers/ ACF Three Columns
---

<Fragment>
  {
    layouts &&
      layouts.map((layout) => {
        const { acf_fc_layout } = layout;

        if (acf_fc_layout === 'one_column') {
          return <OneColumn layout={layout} />;
        }

        if (acf_fc_layout === 'two_columns') {
          return <TwoColumn layout={layout} />;
        }

        if (acf_fc_layout === 'three_columns') {
          return <ThreeColumn layout={layout} />;
        }
        // if (layout.acf_fc_layout === 'accordion') {
        //   return <Accordion key={`acf_layout_${index}`} layout={layout} />;
        // }

        return null;
      })
  }
</Fragment>
